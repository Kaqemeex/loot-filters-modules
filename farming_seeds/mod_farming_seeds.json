{
    "name": "Farming seeds",
    "rs2fPath": "mod_farming_seeds.rs2f",
    "subtitle": "The stuff from the master farmers' pockets",
    "description": "Provides specific seed filters and value threshold for filtering out seeds from being shown. The default is to hide lower tier seeds.",
    "inputs": [
        {
            "macroName": "VAR_FARMING_SEEDS_MINIMUM_VALUE",
            "label": "Minimum value to display secondaries at",
            "type": "number",
            "default": 0
        },
        {
            "macroName": "VAR_FARMING_SEEDS_ALLOTMENT_HIDE",
            "label": "Allotment seeds to hide",
            "type": "enumlist",
            "enum": [
                "Potato seed",
                "Onion seed",
                "Cabbage seed",
                "Tomato seed",
                "Sweetcorn seed",
                "Strawberry seed",
                "Watermelon seed",
                "Snape grass seed"
            ],
            "default": [
                "Potato seed",
                "Onion seed",
                "Cabbage seed",
                "Tomato seed"
            ]
        },
        {
            "macroName": "VAR_FARMING_SEEDS_FLOWER_HIDE",
