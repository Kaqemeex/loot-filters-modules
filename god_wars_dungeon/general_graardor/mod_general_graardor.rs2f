#define VAR_GENERAL_GRAARDOR_UNIQUES_SHOW []

#define VAR_GENERAL_GRAARDOR_COMMON_HIDE []
#define VAR_GENERAL_GRAARDOR_COMMON_SHOW []

#define VAR_GENERAL_GRAARDOR_SUPPLY_HIDE []
#define VAR_GENERAL_GRAARDOR_SUPPLY_SHOW []



#define VAR_DISABLE_GENERAL_GRAARDOR_MODULE 

apply (!VAR_DISABLE_GENERAL_GRAARDOR_MODULE) {

    apply (area:CONST_GENERAL_GRAARDOR_AREA && name:VAR_GENERAL_GRAARDOR_UNIQUES_HIDE) {
        hidden = true;
    }

    apply (area:CONST_GENERAL_GRAARDOR_AREA && name:VAR_GENERAL_GRAARDOR_COMMON_HIDE) {
