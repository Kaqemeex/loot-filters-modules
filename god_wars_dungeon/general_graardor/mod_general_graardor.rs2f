    apply (area:CONST_GENERAL_GRAARDOR_AREA && name:VAR_GENERAL_GRAARDOR_UNIQUES_SHOW) {
        hidden = false;
    }

    apply (area:CONST_GENERAL_GRAARDOR_AREA && name:VAR_GENERAL_GRAARDOR_COMMON_SHOW) {
        hidden = false;
    }

    apply (area:CONST_GENERAL_GRAARDOR_AREA && name:VAR_GENERAL_GRAARDOR_SUPPLY_SHOW) {
        hidden = false;
    }

