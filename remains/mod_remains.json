{
    "name": "Remains",
    "rs2fPath": "mod_remains.rs2f",
    "subtitle": "Don't open dead inside",
    "description": "For filtering out bones, ensouled heads, ashes, and shades of morton remains",
    "inputs": [
        {
            "macroName": "VAR_REMAINS_MINIMUM_VALUE",
            "label": "Minimum value to display remains at",
            "type": "number",
            "default": 0
        },
        {
            "macroName": "VAR_BONES_HIDE",
            "label": "Bones to hide",
            "type": "enumlist",
            "enum": [
                "Bones",
                "Wolf bones",
                "Burnt bones",
                "Monkey bones",
                "Bat bones",
                "Big bones",
                "Jogre bones",
                "Wyrmling bones",
                "Zogre bones",
                "Shaikahan bones",
                "Babydragon bones",
                "Wyrm bones",
                "Wyvern bones",
                "Dragon bones",
                "Drake bones",
                "Fayrg bones",
                "Lava dragon bones",
                "Raurg bones",
                "Hydra bones",
                "Dagannoth bones",
                "Ourg bones",
                "Superior dragon bones",
                "Bleached bones",
                "Small zombie monkey bones",
                "Large zombie monkey bones",
                "Small ninja monkey bones",
                "Medium ninja monkey bones",
                "Gorilla bones",
                "Bearded gorilla bones"
            ],
            "default": [
                "Bones",
                "Wolf bones",
                "Burnt bones",
                "Monkey bones",
                "Bat bones",
                "Big bones",
                "Jogre bones",
                "Wyrmling bones",
                "Zogre bones"
            ]
        },
        {
            "macroName": "VAR_ASHES_HIDE",
