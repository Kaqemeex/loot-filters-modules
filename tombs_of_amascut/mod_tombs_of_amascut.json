        {
            "macroName": "VAR_TOA_SUPPLIES_HIDE",
            "label": "Supplies to hide",
            "group": "TOA Item Filters",
            "type": "enumlist",
            "enum": [
                {
                    "value": "Honey locust (*",
                    "label": "Honey locust"
                },
                {
                    "value": "Ambrosia (*",
                    "label": "Ambrosia"
                },
                {
                    "value": "Blessed crystal scarab (*",
                    "label": "Blessed crystal scarab"
                },
                {
                    "value": "Liquid adrenaline (*",
                    "label": "Liquid adrenaline"
                },
                {
                    "value": "Nectar (*",
                    "label": "Nectar"
                },
                {
                    "value": "Silk dressing (*",
                    "label": "Silk dressing"
                },
                {
                    "value": "Smelling salts (*",
                    "label": "Smelling salts"
                },
                {
                    "value": "Tears of Elidinis (*",
                    "label": "Tears of Elidinis"
                }
            ],
            "default": []
        },
        {
